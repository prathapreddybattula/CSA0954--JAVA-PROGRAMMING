import java.util.Scanner;
public class Fibonacci 
{
    public static void main(String[] args) 
    {
        int n, n1 = 0, n2 = 0, n3= 1;
        Scanner s = new Scanner(System.in);
        System.out.print("Enter value of n:");
        n = s.nextInt();
        System.out.print("Fibonacci Series:");
        for(int i = 1; i <= n; i++)
        {
            n1= n2;
            n2= n3;
            n3= n1 + n2;
            System.out.print(n1+" ");
        }
    }
}
